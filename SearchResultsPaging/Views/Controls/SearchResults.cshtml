@using SearchResultsPaging.Types

@model SearchModel

        @Html.Partial("/Views/Controls/SearchPaging.cshtml", new PagingModel(Model.TotalResults, Model.CurrentPage, Model.PageSize, Model.CurrentMax, Model.CurrentMin))
        <br />

        @foreach (Treasure treasure in Model.TreasureList)
        {
            <div>[@treasure.Id] @treasure.Name - $@treasure.Value</div>
        }
